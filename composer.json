{
    "name": "kataklys/laravel5-playsmsws",
    "description": "PlaySMS API for Laravel 5",
    "type": "library",
    "keywords": ["sms","laravel5","playsms"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Ionut Morar",
            "email": "ionut@infoacademy.net"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "4.*"
    },
    "autoload": {
        "psr-4": {
            "kataklys\\Playsmsws": "src/"
        },
        "files": [
          "src/functions.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "kataklys\\Playsmsws\\PlaysmswsServiceProvider"
            ],
            "aliases": {
                "Playsmsws": "kataklys\\Playsmsws\\Facades\\PlaysmswsFacade"
            }
        }
    }
}